{
  "name": "tourze/workerman-messenger-bundle",
  "description": "Workerman + Symfony Messenger",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-pcntl": "*",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/event-dispatcher": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/messenger": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "workerman/workerman": "^5.1"
  },
  "require-dev": {
    "maglnet/composer-require-checker": "^4",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\WorkermanMessengerBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\WorkermanMessengerBundle\\Tests\\": "tests"
    }
  }
}